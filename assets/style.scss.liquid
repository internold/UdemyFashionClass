h1 {font-size: 32px;}
h2 {font-size: 27px;}
h3 {font-size: 24px;}
h4 {font-size: 21px;}
h5 {font-size: 18px;}
h6 {font-size: 15px;}

ol,ul{margin:0px;}

figure {margin: 0px;}
