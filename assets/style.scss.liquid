.img-responsive{max-width:100%;}

a {color: $SecondaryColor;outline:none;cursor: pointer;@include transitionTime(0.5s);}
button{@include transitionTime(0.5s);cursor: pointer;padding:0;}
button[type='submit']{padding:0;}

a:hover,a:focus,a:active{ color:$PrimaryColor; text-decoration:none;}
  
