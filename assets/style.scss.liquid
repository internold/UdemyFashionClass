/* =====//   Mobile Menu   \\===== */
#nav-icon{width:23px;height:17px;display:none;@include transformRotate(0deg);@include transitionTime(.5s);cursor: pointer;vertical-align:middle;
  margin-left:5px;margin-top:-10px;
  span{display: block;position: absolute;height:3px;width: 50%;background:$blackColor;opacity: 1;@include transformRotate(0deg);@include transitionTime(.25s);
    &:nth-child(even){left: 50%;border-radius:0;}
    &:nth-child(odd){left:0px;border-radius:0;}
    &:nth-child(1),&:nth-child(2) {top: 0px;}
    &:nth-child(3),&:nth-child(4) {top: 8px;}
    &:nth-child(5),&:nth-child(6) {top: 16px;}
  }
  &.open {
    span{
      &:nth-child(1),&:nth-child(6) {@include transformRotate(45deg);}
      &:nth-child(2),&:nth-child(5) {@include transformRotate(-45deg);}
      &:nth-child(1) {left: 1px;top: 4px;}
      &:nth-child(2) {left: calc(50% - 1px);top: 4px;}
      &:nth-child(3) {left: -50%;opacity: 0;}
      &:nth-child(4) {left: 93%;opacity: 0;}
      &:nth-child(5) {left: 1px;top: 13px;}
      &:nth-child(6) {left: calc(50% - 1px);top: 13px;}
    }
  }
}

